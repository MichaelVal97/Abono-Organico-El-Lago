{
    "buildCommand": "npm run build",
    "outputDirectory": ".next",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "framework": "nextjs",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api"
        }
    ],
    "regions": [
        "iad1"
    ],
    "env": {
        "NEXT_PUBLIC_API_URL": "@api_url",
        "NEXT_PUBLIC_CHATBOT_WS": "@chatbot_ws_url"
    }
}